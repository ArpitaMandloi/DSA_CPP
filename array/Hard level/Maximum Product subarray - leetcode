int maxProduct(vector<int>& nums) {
        int maxi = INT_MIN;
        int n = nums.size();
        int product = 1 ;
        int suffix = 1 ;
        int prefix = 1;
        for(int i = 0 ; i<n; i++){
        if (suffix == 0){
            suffix = 1 ;
        }
        if (prefix == 0){
            prefix = 1;
        }
        prefix = prefix * nums[i];
        suffix = suffix * nums[n-i-1];
        maxi = max (maxi,max(suffix , prefix));
    }
    return maxi;
    }
