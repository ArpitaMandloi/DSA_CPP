int missingNumber(vector<int>& nums) {

        int N = nums.size() ;
        int sumExpected = N * (N + 1) / 2; // Sum formula for first N natural numbers
    int sumActual = 0;

    for (int num : nums) {
        sumActual += num; // Sum of given numbers
    }

    return sumExpected - sumActual; // Difference gives the missing number

    }
  
