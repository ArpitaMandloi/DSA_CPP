ListNode* removeNthFromEnd(ListNode* head, int n) {
    ListNode* temp = new ListNode(0);
    temp->next = head;
    ListNode* fast = temp;
    ListNode* slow = temp;

    
    for (int i = 0; i <= n; i++) fast = fast->next;

   
    while (fast != nullptr) {
        fast = fast->next;
        slow = slow->next;
    }

    
    ListNode* delNode = slow->next;
    slow->next = slow->next->next;
    delete delNode;

    head = temp->next;
    delete temp;
    return head;
}
