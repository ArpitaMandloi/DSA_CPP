  ListNode* reverseList(ListNode* head) {
        ListNode* temp = head;
        stack<int> st;
        while(temp != nullptr){
            st.push(temp -> val);
            temp = temp-> next;
        }
        temp = head;
        while(temp != nullptr){
            temp-> val = st.top();
            st.pop();
            temp = temp-> next;
        }
        return head;
    }
